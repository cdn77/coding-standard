{
    "name": "cdn77/coding-standard",
    "description": "CDN77 Coding Standard.",
    "type": "phpcodesniffer-standard",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Moravec",
            "email": "michael@cdn77.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cdn77CodingStandard\\": "Cdn77CodingStandard/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cdn77CodingStandard\\": "tests/"
        },
        "classmap": [
            "vendor/slevomat/coding-standard/tests/Sniffs/TestCase.php"
        ]
    },
    "require": {
        "php": "^7.1",
        "slevomat/coding-standard": "4.0.0",
        "squizlabs/php_codesniffer": "^3.0.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.3"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phing/phing": "^2.16",
        "phpunit/phpunit": "^6.3"
    },
    "config": {
        "preferred-install": "source",
        "sort-packages": true
    }
}
