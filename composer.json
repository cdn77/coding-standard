{
    "name": "cdn77/coding-standard",
    "description": "CDN77 Coding Standard.",
    "type": "phpcodesniffer-standard",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Moravec",
            "email": "michael@cdn77.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cdn77CodingStandard\\": "Cdn77CodingStandard/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cdn77CodingStandard\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "slevomat/coding-standard": "^4.4.6",
        "squizlabs/php_codesniffer": "^3.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phing/phing": "^2.16",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "sort-packages": true
    }
}
